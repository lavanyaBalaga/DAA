#include<stdio.h>
#include<stdlib.h>
int place(int k,int i,int x[])
{
    for(int j=0;j<=k-1;j++)
       if(x[j]==i || abs(j-k)==abs(x[j]-i))
         return 0;
    return 1;
}
void NQueens(int k,int n,int x[])
{
    for(int i=0;i<n;i++)
    {
        if(place(k,i,x))
        {
            x[k]=i;
            if(k==n-1){
                for(int j=0;j<n;j++)  
                 printf("%d ",x[j]+1); 
                 printf("\n");}
            else
              NQueens(k+1,n,x);
        }
    }
}
void main()
{ 
    
    int x[4];
    for(int i=0;i<4;i++)
      x[i]=1;
      NQueens(0,4,x);
}
